{
  "version": 2,
  "framework": null,
 "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }    
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/_profiler/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(css|js|images|fonts)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "dev",
    "APP_DEBUG": "1"
  }
}